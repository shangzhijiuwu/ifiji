<?xml version="1.0" encoding="UTF-8"?>
<ehcache name="fijicache">

    <diskStore path="java.io.tmpdir/fiji-ehcache"/>

    <cache name="FijiCache"
           maxEntriesLocalHeap="2000"
           eternal="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="0"
           overflowToDisk="false"
           statistics="true">
    </cache>

</ehcache>